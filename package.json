{
    "name": "nativescript-colorslider",
    "version": "1.0.0",
    "description": "Color Slider - choose a color!",
    "main": "colorslider.js",
    "typings": "colorslider.d.ts",
    "nativescript": {
        "platforms": {
            "android": "2.0.0",
            "ios": "2.0.0"
        }
    },
    "scripts": {
        "build": "tsc",
        "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
        "demo.android": "npm run preparedemo && cd demo && tns run android",
        "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-colorslider && tns plugin add .. && tns install",
        "setup": "cd demo && npm install && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jlooper/nativescript-colorslider.git"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
